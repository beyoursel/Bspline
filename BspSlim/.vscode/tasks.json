{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "catkin_make:release",
            "type": "shell",
            "command": "catkin_make",
            "args": [
                "-DCMAKE_BUILD_TYPE=Release",
                "-DPYTHON_EXECUTABLE=/usr/bin/python3"
            ],
            "group": "build",
            "presentation": {
                "reveal": "always"
            },
            "problemMatcher": "$msCompile"
        },
    ]
}
